  <% if @presenter.identifiers_minted?(:doi) %>
  <tr>
    <th><%= t('simple_form.labels.generic_work.doi') %></th>
    <td>
      <ul class="tabular">
        <li class="attribute">
            <% status = @presenter.identifiers_pending?(:doi) ? 'warning' : 'default' %>
            <span class="label label-<%= status %>"><%= @presenter.doi %></span>
        </li>
      </ul>
    </td>
  </tr>
  <% end %>